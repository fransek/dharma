{
  "name": "dharma-react",
  "version": "0.8.2",
  "author": "Frans Ekman <fransedvinekman@gmail.com>",
  "license": "MIT",
  "description": "React bindings for Dharma",
  "keywords": [
    "state",
    "store",
    "state management",
    "redux",
    "react"
  ],
  "homepage": "https://github.com/fransek/dharma/tree/main/packages/dharma-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fransek/dharma/tree/main/packages/dharma-react"
  },
  "bugs": {
    "url": "https://github.com/fransek/dharma/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/esm/lib/*.d.ts",
        "default": "./dist/esm/lib/*.js"
      },
      "require": {
        "types": "./dist/cjs/lib/*.d.ts",
        "default": "./dist/cjs/lib/*.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage --passWithNoTests",
    "lint": "eslint"
  },
  "publishConfig": {
    "provenance": true
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "dharma-core": "workspace:*",
    "react": "^19.1.1"
  },
  "peerDependencies": {
    "react": "^19.1.1"
  }
}
