{
  "name": "dharma-core",
  "version": "0.10.0",
  "author": "Frans Ekman <fransedvinekman@gmail.com>",
  "license": "MIT",
  "description": "A simple and lightweight state management solution for JavaScript and TypeScript applications.",
  "keywords": [],
  "homepage": "https://github.com/fransek/dharma/tree/main/packages/dharma-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fransek/dharma/tree/main/packages/dharma-core"
  },
  "bugs": {
    "url": "https://github.com/fransek/dharma/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/esm/lib/*.d.ts",
        "default": "./dist/esm/lib/*.js"
      },
      "require": {
        "types": "./dist/cjs/lib/*.d.ts",
        "default": "./dist/cjs/lib/*.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage --passWithNoTests",
    "lint": "eslint"
  },
  "publishConfig": {
    "provenance": true
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0"
  }
}
