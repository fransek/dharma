{
  "name": "dharma-core",
  "version": "0.7.1",
  "author": "Frans Ekman <fransedvinekman@gmail.com>",
  "license": "MIT",
  "description": "My package description",
  "keywords": [],
  "homepage": "https://github.com/fransek/dharma/tree/main/packages/dharma-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fransek/dharma/tree/main/packages/dharma-core"
  },
  "bugs": {
    "url": "https://github.com/fransek/dharma/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./createStore": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/lib/createStore.js",
      "require": "./dist/cjs/lib/createStore.js"
    },
    "./createPersistentStore": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/lib/createPersistentStore.js",
      "require": "./dist/cjs/lib/createPersistentStore.js"
    },
    "./merge": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/lib/merge.js",
      "require": "./dist/cjs/lib/merge.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage --passWithNoTests",
    "lint": "eslint"
  },
  "publishConfig": {
    "provenance": true
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0"
  }
}
